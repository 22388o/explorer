<?xml version="1.1" encoding="UTF-8" standalone="no"?>
<databaseChangeLog xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                   xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
                   xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd">
    <changeSet author="straumat" id="0.1.0-table-constraints-requests_add_proof">

        <!-- Foreign key REQUESTS_ADD_ASSET.ID > REQUESTS.ID -->
        <addForeignKeyConstraint constraintName="FK_REQUESTS_ADD_PROOF_REQUESTS_ID"
                                 baseColumnNames="ID"
                                 baseTableName="REQUESTS_ADD_PROOF"
                                 referencedTableName="REQUESTS"
                                 referencedColumnNames="ID"
                                 validate="true"/>

        <!-- Foreign key FK_ASSET > ASSETS.ID -->
        <addForeignKeyConstraint constraintName="FK_REQUESTS_ADD_PROOF_ASSET_ID"
                                 baseColumnNames="FK_ASSET"
                                 baseTableName="REQUESTS_ADD_PROOF"
                                 referencedTableName="ASSET"
                                 referencedColumnNames="ID"
                                 validate="true"/>

    </changeSet>
</databaseChangeLog>